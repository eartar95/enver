projection;
strict ( 2 );
use draft;
use side effects;

define behavior for ZEA_C_JOURNAL //alias <alias_name>
{
  use create;
  use update;
  use delete;

  use action Resume;
  use action Edit;
  use action Activate;
  use action Discard;
  use action Prepare;

  use action UploadXlsx;

  use association _Item {  with draft; }
}

define behavior for ZEA_C_JOURNAL_ITEM //alias <alias_name>
{
  use update;
  use delete;

  use association _Header { with draft; }
}